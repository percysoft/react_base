  "scripts": {
    "start": "env-cmd ./environments/.env.development react-app-rewired start",
    "buildOnly:develop": "env-cmd ./environments/.env.development react-app-rewired build && mkdir -p dist/develop && mv build/* dist/develop",
    "buildOnly:staging": "env-cmd ./environments/.env.qa react-app-rewired build && mkdir -p dist/qa && mv build/* dist/qa",
    "buildOnly:production": "env-cmd ./environments/.env.production react-app-rewired build && mkdir -p dist/production && mv build/* dist/production",
    "build:develop": "env-cmd ./environments/.env.development react-app-rewired build && rm -rf dist/develop && mkdir -p dist/develop && mv build/* dist/develop",
    "build:staging": "env-cmd ./environments/.env.qa react-app-rewired build && rm -rf dist/qa &&  mkdir -p dist/qa && mv build/* dist/qa",
    "build:production": "env-cmd ./environments/.env.production react-app-rewired build && rm -rf dist/production &&  mkdir -p dist/production && mv build/* dist/production",
    "build": "rm -rf dist && run-s buildOnly:*",
    "test": "env-cmd ./environments/.env.development react-app-rewired test",
    "coverage": "env-cmd ./environments/.env.development react-app-rewired test src/ --coverage",
    "eject": "react-app-rewired eject",
    "icons": "node ./icons",
    "storybook": "start-storybook -p 3200 -c .storybook",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
